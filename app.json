{
  "expo": {
    "name": "SmartRide Manager",
    "slug": "smartride-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartride.manager",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to track your rides.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to track your rides in the background."
      },
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_IOS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smartride.manager",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_ANDROID_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "victory-native"
          ]
        }
      },
      "config": {
        "firebase": {
          "measurementId": "YOUR_MEASUREMENT_ID"
        }
      },
      "backgroundColor": "#ffffff",
      "themeColor": "#4285F4",
      "name": "SmartRide Manager",
      "shortName": "SmartRide",
      "description": "Track and manage everything about your bike rides - from maintenance and services to fuel expenses, mileage, and ride records.",
      "lang": "en",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SmartRide Manager to use your location to track rides."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}